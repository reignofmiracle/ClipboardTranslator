{
  "name": "clipboardtranslator",
  "productName": "ClipboardTranslator",
  "version": "1.5.0",
  "description": "simple clipboard translator",
  "main": "dist/main/index.js",
  "scripts": {
    "test": "eslint src/**/*.js src/**/*.jsx",
    "compile": "webpack",
    "watch": "webpack --watch",
    "start": "electron .",
    "build:osx": "build --mac",
    "build:linux32": "build --linux --ia32",
    "build:linux64": "build --linux --x64",
    "build:win32": "build --win --ia32",
    "build:win64": "build --win --x64"
  },
  "keywords": [
    "Electron",
    "clipboard",
    "translator"
  ],
  "homepage": "https://github.com/reignofmiracle/ClipboardTranslator",
  "author": "reignofmiracle@gmail.com",
  "license": "MIT",
  "build": {
    "productName": "ClipboardTranslator",
    "appId": "com.beycat.ClipboardTranslator",
    "asar": true,
    "protocols": {
      "name": "ClipboardTranslator",
      "schemes": [
        "ClipboardTranslator"
      ]
    },
    "mac": {
      "target": [
        "default"
      ],
      "icon": "./icon/ClipboardTranslator.icns"
    },
    "dmg": {
      "title": "ClipboardTranslator",
      "icon": "./icon/ClipboardTranslator.icns"
    },
    "win": {
      "target": [
        "zip",
        "nsis",
        "portable"
      ],
      "icon": "./icon/ClipboardTranslator.ico"
    },
    "linux": {
      "target": [
        "AppImage",
        "deb",
        "rpm",
        "zip",
        "tar.gz"
      ],
      "icon": "./resources"
    },
    "nsis": {
      "oneClick": false,
      "allowToChangeInstallationDirectory": true
    },
    "directories": {
      "buildResources": "icon/",
      "output": "release/",
      "app": "./"
    }
  },
  "dependencies": {
    "lodash": "^4.17.5",
    "npm": "^5.7.1",
    "react": "^16.2.0",
    "react-dom": "^16.2.0",
    "rxjs": "^5.5.7"
  },
  "devDependencies": {
    "babel-core": "^6.26.0",
    "babel-loader": "^7.1.4",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "css-loader": "^0.28.10",
    "electron": "^1.8.3",
    "electron-builder": "^20.4.1",
    "electron-json-storage": "^4.0.2",
    "eslint": "^4.18.2",
    "eslint-plugin-react": "^7.7.0",
    "style-loader": "^0.20.3",
    "webpack": "^4.1.1",
    "webpack-cli": "^2.0.11"
  }
}
